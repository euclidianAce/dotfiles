! URxvt extensions
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: False

URxvt.perl-ext-common: default,resize-font

*locale: en_US.UTF-8

/* *.font: xft:Terminus (TTF):size=12:antialias=true */
*.font: xft:Ubuntu Mono:size=12:antialias=true
/* *.font: xft:Hasklig:size=11:antialias=true */
/* *.font: -*-terminus-*-*-*-*-16-*-*-*-*-*-*-* */
/* *.font = xft:Julia Mono:size=12:antialias=true */
/* *.font = xft:Fixedsys Excelsior */

*.letterSpace: 1
*.lineSpace: 0
*.scrollBar: false
*.internalBorder: 10
*.cursorBlink: true
URxvt.pointerBlank: true

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282a36
*.color0: #000000
*.color8: #4D4D4D
*.color1: #FF5555
*.color9: #FF6E67
*.color2: #50FA7B
*.color10: #5AF78E
*.color3: #F1FA8C
*.color11: #F4F99D
*.color4: #BD93F9
*.color12: #CAA9FA
*.color5: #FF79C6
*.color13: #FF92D0
*.color6: #8BE9FD
*.color14: #9AEDFE
*.color7: #BFBFBF
*.color15: #E6E6E6
