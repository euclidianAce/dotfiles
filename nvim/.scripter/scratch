local z = libreq("async.zig")
local menu = plugreq("package-manager.menu", true)
local function o(s)
	return {s, function() print(s) end}
end
local acc = menu.new.accordion{
	o 'a',
	o 'b',
	{'C', {
		o 'd',
		o 'e',
		{'F', {
			o 'g',
			o 'h',
		}},
	}},
}


z.async(acc.run, acc, {
	centered = true,
	wid = 30,
	hei = 10,
	interactive = true,
	ephemeral = true,
})
